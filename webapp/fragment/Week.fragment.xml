<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog multiSelect="true"
		title="Week"
		class="sapUiPopupWithPadding"
		items="{/WeekF4Set}"
		search="handleValueHelpSearchWeek"
		confirm="handleValueHelpConfWeek"
		cancel="handleValueHelpClose" growingThreshold="true" growing="true" >
		<StandardListItem
			title="{Zweek}"
		    description="{Zyear}" 
			info="{path: 'Zdatefr', formatter: '.formatDate'} - {path: 'Zdateto', formatter: '.formatDate'}"
			/>
	</SelectDialog>
</core:FragmentDefinition>